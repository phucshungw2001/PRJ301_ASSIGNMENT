<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.Student.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>Controller.Student.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentController</servlet-name>
        <servlet-class>Controller.Student.StudentController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GroupController</servlet-name>
        <servlet-class>Controller.Student.GroupController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutController</servlet-name>
        <servlet-class>Controller.Student.LogOutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubjectsController</servlet-name>
        <servlet-class>Controller.Student.SubjectsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkController</servlet-name>
        <servlet-class>Controller.Student.MarkController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkViewController</servlet-name>
        <servlet-class>Controller.Student.MarkViewController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AssessmentController</servlet-name>
        <servlet-class>Controller.Student.AssessmentController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LecturersSubjectsController</servlet-name>
        <servlet-class>Controller.Lecturers.LecturersSubjectsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LecturersController</servlet-name>
        <servlet-class>Controller.Lecturers.LecturersController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GroupLecturersController</servlet-name>
        <servlet-class>Controller.Lecturers.GroupLecturersController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentLecturersController</servlet-name>
        <servlet-class>Controller.Lecturers.StudentLecturersController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkLecturersController</servlet-name>
        <servlet-class>Controller.Lecturers.MarkLecturersController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentController</servlet-name>
        <url-pattern>/student</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GroupController</servlet-name>
        <url-pattern>/group</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubjectsController</servlet-name>
        <url-pattern>/subject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkController</servlet-name>
        <url-pattern>/mark</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkViewController</servlet-name>
        <url-pattern>/markview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AssessmentController</servlet-name>
        <url-pattern>/assessment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LecturersSubjectsController</servlet-name>
        <url-pattern>/lecturerssubjects</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LecturersController</servlet-name>
        <url-pattern>/lecturers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GroupLecturersController</servlet-name>
        <url-pattern>/grouplecturers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentLecturersController</servlet-name>
        <url-pattern>/studentlecturers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkLecturersController</servlet-name>
        <url-pattern>/marklecturers</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
